    "def adequa_dataset(path_excel: str) -> Tuple[pd.DataFrame, List[str], str]:\n",
    "    \"\"\"\n",
    "    Está função adequa um conjunto de dados em Excel para o formato com colunas sem mesclas e extrai os nomes dos elementos estruturais, como da família.\n",
    "\n",
    "    :param path_excel: Caminho do arquivo Excel.\n",
    "\n",
    "    :return:\n",
    "        - df_ajustado: Conjunto de dados sem colunas mescladas (ex: coluna nova -> \"Fi - Elemento 1\").\n",
    "        - nome_elementos: Nomes dos elementos estruturais.\n",
    "        - nome_familia: Nome da familia de elementos.\n",
    "    \"\"\"\n",
    "\n",
    "    # Ajuste do header\n",
    "    df_ajustado = pd.read_excel(path_excel, header=[0, 1])\n",
    "    elementos_brutos = df_ajustado.columns.get_level_values(0)\n",
    "    \n",
    "    # Extração dos nomes dos elementos estruturais\n",
    "    nome_elementos: List[str] = sorted(set(e for e in elementos_brutos if e != 'Danos'))\n",
    "    \n",
    "    # Ajuste do DataFrame\n",
    "    df_ajustado.columns = [\n",
    "        f\"{sub} - {main}\" if main != 'Danos' else 'Danos'\n",
    "        for main, sub in df_ajustado.columns\n",
    "    ]\n",
    "\n",
    "    # Nome do conjunto de elemenntos\n",
    "    nome_familia = os.path.splitext(os.path.basename(path_excel))[0]\n",
    "\n",
    "    # Preenchendo valores NaN com 0\n",
    "    df_ajustado.fillna(0, inplace=True)\n",
    "\n",
    "    return df_ajustado, nome_elementos, nome_familia\n",
    "\n",
    "df_ajustado, nome_elementos, nome_familia = adequa_dataset('planilhas_teste/pilares.xlsx')\n",
    "print(nome_elementos)\n",
    "print(nome_familia)\n",
    "df_ajustado"
   ]
