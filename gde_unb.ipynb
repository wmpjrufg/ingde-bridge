    "    return {nome_familia: {'gde_max': gde_max, 'g_df': float(g_df), 'f_r': f_r, 'f_r × g_df': float(fr_gdf)}}\n",
    "\n",
    "resumo_familia = avalia_familia(df_ajustado, nome_elementos, nome_familia, 5)\n",
    "resumo_familia"
